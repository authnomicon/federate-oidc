<head>
  <title>Check Origin IFrame</title>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js" type="text/javascript"></script>
</head>
<body>
	<script type="text/javascript">
    
    jQuery(function() {
      console.log('check origin iframe ready...')
      
      var cookie = true;
      
      var n = 'CheckConnectionTempCookie' + String(Math.floor(1000 * Math.random()));
      var val = String(Math.floor(1000000 * Math.random()));
      var exp = new Date((new Date).getTime() + 10000); // 10 seconds from now
    
      document.cookie = n + '=' + val + '; Expires=' + exp.toUTCString();
      if (document.cookie.indexOf(n + '=' + val) == -1) {
        cookie = false;
      }
      
      window.parent.postMessage({ type: 'origin_info', info: { cookie: cookie } }, 'http://localhost:3001');
    });
    
	</script>
</body>
    